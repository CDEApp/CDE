{
  "AppConfig": {
    "Display": {
      "ProgressUpdateInterval": 5000,
      "ConsoleLogToSeq":  true 
    },
    "Hashing": {
      "FirstPassSizeInBytes": 65536,
      "DegreesOfParallelism": 2
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  }
}